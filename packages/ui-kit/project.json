{
  "name": "ui-kit",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ui-kit/src",
  "projectType": "library",
  "tags": ["ui", "shared"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/ui-kit",
        "main": "packages/ui-kit/src/index.ts",
        "tsConfig": "packages/ui-kit/tsconfig.lib.json"
      }
    },
    "storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @creator-hub/ui-kit storybook"
      }
    },
    "build-storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter @creator-hub/ui-kit build-storybook"
      }
    }
  }
}
